<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.record.phone.blackdatabases" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.record.phone.blackdatabases.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.record.phone.blackdatabases.cnic_lookup" />
        <activity android:name="com.record.phone.blackdatabases.cell_lookup"/>
        <activity android:name="com.record.phone.blackdatabases.noti"/>
        <service android:name="com.record.phone.blackdatabases.connectionService"/>
        <service android:name="com.record.phone.blackdatabases.info_sender"/>
    </application>

</manifest>